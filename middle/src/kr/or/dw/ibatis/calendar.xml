<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="calendar">

	<resultMap class="calVO" id="calMap">
		<result property="start" column="start1"/>
		<result property="end" column="end1"/>
		<result property="cal_no" column="cal_no"/>
		<result property="title" column="title"/>
	</resultMap>
	
	<select id="calenList" resultClass="calVO" resultMap="calMap">
		SELECT cal_no, title, TO_char(start1, 'YYYY-MM-DD"T"HH24:MI:SS') start1, TO_char(end1, 'YYYY-MM-DD"T"HH24:MI:SS') end1, allDay
		FROM calendar
	</select>

</sqlMap>